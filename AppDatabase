package com.example.lastproject.db;

import androidx.room.Database;
import androidx.room.RoomDatabase;
import androidx.room.TypeConverters;

import com.example.lastproject.User;
import com.example.lastproject.UserLog;

@Database(entities = {UserLog.class, User.class}, version = 2)
@TypeConverters(DateTypeConverter.class)
public abstract class AppDatabase extends RoomDatabase {
    public static final String DB_NAME = "USERLOG_DATABASE";
    public static final String USERLOG_TABLE = "USERLOG_TABLE";
    public static final String USER_TABLE = "USER_TABLE";


    public abstract UserLogDAO getUserLogDAO();
}
